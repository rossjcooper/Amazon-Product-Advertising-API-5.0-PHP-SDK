{
    "name" : "rossjcooper/paapiphpsdk",
    "description": "PHP SDK that allows you to access the Amazon Product Advertising API 5.0 from your PHP app",
    "version" : "1.0.0",
    "keywords" : [
        "amazon",
        "php",
        "sdk",
        "api"
    ],
    "homepage" : "https://webservices.amazon.com/paapi5/documentation/index.html",
    "license" : "Apache-2.0",
    "authors" : [{
        "name" : "Product Advertising API Team"
    }
    ],
    "require" : {
        "php" : ">=5.5",
        "guzzlehttp/guzzle" : "^6.2"
    },
    "autoload": {
        "psr-4": { "Amazon\\ProductAdvertisingAPI\\v1\\" : "src/" }
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.5"
    },
    "config": {
        "platform": {
            "php": ">=7.1"
        }
    }
}
